digraph dustviz {
  rankdir=LR;
  node [shape=box, fontname="monospace"];
  edge [fontname="monospace"];

  n0 [label="Program", shape=oval];
  n1 [label="Forge\ncore", shape=folder];
  n2 [label="Proc [K]\nmain", shape=component];
  n3 [label="Stmt\nEffect emit \"Hello, Dust!\"", shape=box];
  n4 [label="Stmt\nReturn 0", shape=box];

  n5 [label="Clause\nconstraint x > 0 Predicate", shape=parallelogram];
  n6 [label="Clause\nconstraint non_negative_int Rule", shape=parallelogram];

  n0 -> n1 [label="contains"];
  n1 -> n2 [label="contains"];
  n2 -> n3 [label="contains"];
  n2 -> n4 [label="contains"];
  n3 -> n4 [label="next"];

  n6 -> n5 [label="contains"];
  n5 -> n3 [label="clause"];
}
