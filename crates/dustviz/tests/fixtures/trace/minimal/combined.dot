  n6 [label="Trace\nStmtExec main:0", shape=octagon];
  n7 [label="Trace\nStmtExec main:1", shape=octagon];
  n8 [label="Trace\nConstraintFailed C1\nunproved", shape=octagon];

  n0 -> n1 [label="contains"];
  n1 -> n2 [label="contains"];
  n2 -> n3 [label="contains"];
  n2 -> n4 [label="contains"];
  n3 -> n4 [label="next"];
  n5 -> n2 [label="trace"];
  n6 -> n3 [label="trace"];
  n7 -> n4 [label="trace"];
}
